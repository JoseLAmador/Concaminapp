                  _react3.default.createElement(
                    _reactNative.Text,
                    {
                      __source: {
                        fileName: _jsxFileName,
                        lineNumber: 196
                      }
                    },
                    'Comentarios'
                  )
                )
              )
            )
          )
        )
      );
    }
  }]);

  return Perfil;
}(_react2.Component));

exports.default = Perfil;


